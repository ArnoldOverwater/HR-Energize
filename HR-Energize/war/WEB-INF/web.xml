<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>HR_Energize</servlet-name>
		<servlet-class>nl.hr.energize.HR_EnergizeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>HR_Energize</servlet-name>
		<url-pattern>/hr_energize</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>clean</servlet-name>
		<servlet-class>nl.hr.energize.cleanServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>clean</servlet-name>
		<url-pattern>/clean</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>refill</servlet-name>
		<servlet-class>nl.hr.energize.refillServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>refill</servlet-name>
		<url-pattern>/refill</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
